{
  "name": "@mxweb/utils",
  "version": "0.0.4",
  "description": "A comprehensive collection of TypeScript utilities for modern web development",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "MXWeb Team <mxwebio@gmail.com>",
  "homepage": "https://github.com/mxwebio/mxweb-utils#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mxwebio/mxweb-utils.git"
  },
  "bugs": {
    "url": "https://github.com/mxwebio/mxweb-utils/issues"
  },
  "keywords": [
    "utilities",
    "typescript",
    "storage",
    "helpers",
    "utils",
    "lodash",
    "axios",
    "localStorage",
    "sessionStorage",
    "cookies",
    "chunk",
    "flatten",
    "format",
    "interpolate",
    "mxweb"
  ],
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && yarn lint && rollup -c",
    "build:watch": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "1.6.0",
    "@vitest/ui": "1.6.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^11.0.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "rollup": "^4.52.5",
    "rollup-plugin-dts": "^6.2.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "1.6.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./chunk": {
      "types": "./dist/chunk.d.ts",
      "import": "./dist/chunk.mjs",
      "require": "./dist/chunk.js",
      "default": "./dist/chunk.mjs"
    },
    "./flatten": {
      "types": "./dist/flatten.d.ts",
      "import": "./dist/flatten.mjs",
      "require": "./dist/flatten.js",
      "default": "./dist/flatten.mjs"
    },
    "./format-size": {
      "types": "./dist/format-size.d.ts",
      "import": "./dist/format-size.mjs",
      "require": "./dist/format-size.js",
      "default": "./dist/format-size.mjs"
    },
    "./get-env": {
      "types": "./dist/get-env.d.ts",
      "import": "./dist/get-env.mjs",
      "require": "./dist/get-env.js",
      "default": "./dist/get-env.mjs"
    },
    "./get-settled-value": {
      "types": "./dist/get-settled-value.d.ts",
      "import": "./dist/get-settled-value.mjs",
      "require": "./dist/get-settled-value.js",
      "default": "./dist/get-settled-value.mjs"
    },
    "./interpolate": {
      "types": "./dist/interpolate.d.ts",
      "import": "./dist/interpolate.mjs",
      "require": "./dist/interpolate.js",
      "default": "./dist/interpolate.mjs"
    },
    "./pascal-to-kebab": {
      "types": "./dist/pascal-to-kebab.d.ts",
      "import": "./dist/pascal-to-kebab.mjs",
      "require": "./dist/pascal-to-kebab.js",
      "default": "./dist/pascal-to-kebab.mjs"
    },
    "./rate-limiter": {
      "types": "./dist/rate-limiter.d.ts",
      "import": "./dist/rate-limiter.mjs",
      "require": "./dist/rate-limiter.js",
      "default": "./dist/rate-limiter.mjs"
    },
    "./request-timeout": {
      "types": "./dist/request-timeout.d.ts",
      "import": "./dist/request-timeout.mjs",
      "require": "./dist/request-timeout.js",
      "default": "./dist/request-timeout.mjs"
    },
    "./retry": {
      "types": "./dist/retry.d.ts",
      "import": "./dist/retry.mjs",
      "require": "./dist/retry.js",
      "default": "./dist/retry.mjs"
    },
    "./sleep": {
      "types": "./dist/sleep.d.ts",
      "import": "./dist/sleep.mjs",
      "require": "./dist/sleep.js",
      "default": "./dist/sleep.mjs"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "import": "./dist/storage.mjs",
      "require": "./dist/storage.js",
      "default": "./dist/storage.mjs"
    },
    "./uri": {
      "types": "./dist/uri.d.ts",
      "import": "./dist/uri.mjs",
      "require": "./dist/uri.js",
      "default": "./dist/uri.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "default": "./dist/types.mjs"
    },
    "./package.json": "./package.json"
  }
}
